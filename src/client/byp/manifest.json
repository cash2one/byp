{
  "manifest_version": 2,

  "name": "S.W.自助打包服务",
  "version": "1.0",
  "description": "S.W.实时自助打包服务",

  "permissions": [
    "http://localhost"
  ],
  "browser_action": {
    "default_icon": "static/img/icon.png",
    "default_popup": "popup.html"
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost;object-src 'self' 'unsafe-eval' http://localhost"

}